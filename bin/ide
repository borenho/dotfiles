#!/bin/sh

# Creates 5 panes for me
tmux split-window -v -p 50          # Split horizontally
tmux split-window -h -p 66          # In the new pane, split vertically
tmux split-window -h -p 50          # In the new pane, split into two halves vertically

tmux select-pane -t 1               # Go to the first pane
tmux split-window -h -p 66   # Split into two halves vertically and run 'nvim'
tmux split-window -h -p 50

# tmux split-window -p 66 \; split-window -d \; split-window -h
